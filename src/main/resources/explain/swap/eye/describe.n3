@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix pml: <http://provenanceweb.org/ns/pml#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix string: <http://www.w3.org/2000/10/swap/string#> .
@prefix r: <http://www.w3.org/2000/10/swap/reason#> .
@prefix : <http://wvw.org/xai#> .
@base <http://wvw.org/xai#> .


{	?value :term ?t . 
	?t log:rawType log:Other . 
	<> log:notIncludes { ?t rdfs:label ?l } .
	?t log:uri ?uri .
 	( ?uri ".*?#(.*)" ) string:scrape ?str .

} => { ?t :description ?str } .


{ 	?value :term ?t .
	?t log:rawType log:Other .
	?t rdfs:label ?str .

} => { ?t :description ?str } .


{ 	?value :term ?t .
	?t log:rawType log:Literal .

} => { ?t :description ?t } .


{	?x r:gives ?value . ?value log:includes { ?s ?p ?o } .

} => { ?value :term ?s , ?p , ?o } .

 
{	?x r:gives ?value .
	?value log:includes { ?s ?p ?o } .
	
	?s :description ?sd .
	?p :description ?pd .
	?o :description ?od .
	( ?sd " " ?pd " " ?od ) string:concatenation ?str .
	
} => { ?x :description ?str } .