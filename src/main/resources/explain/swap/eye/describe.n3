@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix pml: <http://provenanceweb.org/ns/pml#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix string: <http://www.w3.org/2000/10/swap/string#> .
@prefix r: <http://www.w3.org/2000/10/swap/reason#> .
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix x: <http://wvw.org/xai#> .
@base <http://wvw.org/xai#> .


{	?value x:term ?t . 
	?t log:rawType log:Other . 
	<> log:notIncludes { ?t rdfs:label ?l } .
	?t log:uri ?uri .
 	( ?uri ".*?#(.*)" ) string:scrape ?str .

} => { ?t x:description ?str } .


{ 	?value x:term ?t .
	?t log:rawType log:Other .
	?t rdfs:label ?str .

} => { ?t x:description ?str } .


{ 	?value x:term ?t .
	?t log:rawType log:Literal .

} => { ?t x:description ?t } .


{	?x r:gives ?value . ?value log:includes { ?s ?p ?o } .

} => { ?value x:term ?s , ?p , ?o } .

 
{	?x r:gives ?value .
	?value log:includes { ?s ?p ?o } .
	
	?s x:description ?sd .
	?p x:description ?pd .
	?o x:description ?od .
	( ?sd " " ?pd " " ?od ) string:concatenation ?str .
	
} => { ?x x:conclusionStr ?str } .


{	?inf a x:InferenceNode 
		
} <= { ?inf a r:Inference .
	_:x log:notIncludes { ?x r:component ?inf } 
} .


{	?x a x:InferenceNode ; 
		r:rule ?extraction .
	
	?extraction!r:gives log:localN3String ?ruleStr . 
	
	?extraction!r:because!r:source log:uri ?source .
	
	( ?assign
	{	?x r:binding ?binding . ?binding!r:variable n3:uri ?var . ?binding!r:boundTo n3:uri ?uri .
		( ?var ".*#(.*)" ) string:scrape ?var2 .
		( "var:" ?var2 " = " ?uri " , " ) string:concatenation ?assign .
	} ?uriAssigns) log:collectAllIn _:x .
	
	( ?assign
	{	?x r:binding ?binding . ?binding!r:variable n3:uri ?var . 
		?binding r:boundTo ?lit . ?lit log:rawType log:Literal .
		( ?var ".*#(.*)" ) string:scrape ?var2 .
		( "var:" ?var2 " = " ?lit " , " ) string:concatenation ?assign .
	} ?litAssigns) log:collectAllIn _:x .
	
	( ?uriAssigns ?litAssigns ) list:append ?assigns .
	?assigns string:concatenation ?bindingStr .
	( ?bindingStr "(.*) , $" ) string:scrape ?bindingStr2 .
	
	
} => { ?x x:ruleStr ?ruleStr ; x:sourceStr ?source ; x:bindingStr ?bindingStr2 } .