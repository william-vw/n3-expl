@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix pml: <http://provenanceweb.org/ns/pml#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix string: <http://www.w3.org/2000/10/swap/string#> .
@prefix r: <http://www.w3.org/2000/10/swap/reason#> .
@prefix e: <http://eulersharp.sourceforge.net/2003/03swap/log-rules#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix x: <http://wvw.org/xai#> .
@base <http://wvw.org/xai#> .


{   ?x r:gives ?value . 
	?value e:labelvars ?valuel.
	?valuel log:includes { ?s ?p ?o } .

} => { ?valuel x:term ?s , ?p , ?o } .


{   ?value x:term ?t .
    ?t log:rawType log:Other .
	_:x log:notIncludes { ?t rdfs:label ?l } .

    ?t log:uri ?uri .
    ( ?uri ".*?#(.*)" ) string:scrape ?ln .

    ?t log:localN3String ?ts .

} => { ?ts x:description ?ln } .


{   ?value x:term ?t .
    ?t log:rawType log:Other .
	?t rdfs:label ?l .
	
	?t log:localN3String ?ts .
	
} => { ?ts x:description ?l } .


{ 	?value x:term ?t .
	?t log:rawType log:Literal .

	?t log:localN3String ?ts .

} => { ?ts x:description ?t } .


{   ?x!rdf:type list:in ( r:Extraction r:Inference ) .
    ?x r:gives ?value .
    ?value e:labelvars ?valuel .

    ( ?str
    {
        ?valuel e:graphMember ?m .
        ?m log:includes { ?s ?p ?o } .
        
        ?s!log:localN3String x:description ?sd .
        ?p!log:localN3String x:description ?pd .
        ?o!log:localN3String x:description ?od .
        ( ?sd " " ?pd " " ?od " , " ) string:concatenation ?str .

    } ?strs ) log:collectAllIn _:x .
    
    ?strs string:concatenation ?str .
    ( ?str "(.*) , $" ) string:scrape ?str2 . 
    

} => { ?x x:conclusionStr ?str2 } .


{	?inf a x:InferenceNode 
		
} <= { ?inf a r:Inference .
	_:x log:notIncludes { ?x r:component ?inf } 
} .


{	?x a x:InferenceNode ; 
		r:rule ?extraction .
	
	?extraction!r:gives log:localN3String ?ruleStr .
	( ?ruleStr "(\\{.*?\\}\\s*=>\\s*\\{.*?\\})" ) string:scrape ?ruleStr2 . 
	 
	?extraction!r:because!r:source log:uri ?source .
	
	( ?assign
	{	?x r:binding ?binding . ?binding!r:variable n3:uri ?var . ?binding!r:boundTo n3:uri ?uri .
		( ?uri ".*?#(.*)" ) string:scrape ?ln .
		( ?var ".*#(.*)" ) string:scrape ?var2 .
		( "var:" ?var2 " = " ?ln " , " ) string:concatenation ?assign .
	} ?uriAssigns) log:collectAllIn _:x .
	
	( ?assign
	{	?x r:binding ?binding . ?binding!r:variable n3:uri ?var . 
		?binding r:boundTo ?lit . ?lit log:rawType log:Literal .
		( ?var ".*#(.*)" ) string:scrape ?var2 .
		( "var:" ?var2 " = " ?lit " , " ) string:concatenation ?assign .
	} ?litAssigns) log:collectAllIn _:x .
	
	( ?uriAssigns ?litAssigns ) list:append ?assigns .
	?assigns e:sort ?assigns2 .
	
	?assigns2 string:concatenation ?bindingStr .
	( ?bindingStr "(.*) , $" ) string:scrape ?bindingStr2 .
	
} => { ?x x:ruleStr ?ruleStr2 ; x:sourceStr ?source ; x:bindingStr ?bindingStr2 } .