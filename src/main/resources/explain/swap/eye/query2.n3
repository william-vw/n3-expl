@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix pml: <http://provenanceweb.org/ns/pml#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix string: <http://www.w3.org/2000/10/swap/string#> .
@prefix r: <http://www.w3.org/2000/10/swap/reason#> .
@prefix x: <http://wvw.org/xai#> .
@base <http://wvw.org/xai#> .

{	1 log:outputString ?doc

} <= { ?inf a x:InferenceNode .
	
	( ?x { ?x a x:InferenceNode . ?x!r:evidence list:member ?inf } ?xs ) log:collectAllIn _:x .
	?xs!list:length math:equalTo 0 .
	
	?inf x:output ?html .
	
	( "<html>"
		"<head>"
			"<link rel='stylesheet' href='popup.css' />"
			"<script type='text/javascript' src='popup.js'></script>"
		"</head>"
		"<body onload='loaded()'>" ?html "</body>"
	"</html>"
		
	) string:concatenation ?doc .
} .

#{	?x x:ruleStr ?ruleStr ; x:sourceStr ?sourceStr ; x:bindingStr ?bindingStr 
#} <= { ?x x:ruleStr ?ruleStr ; x:sourceStr ?sourceStr ; x:bindingStr ?bindingStr } .